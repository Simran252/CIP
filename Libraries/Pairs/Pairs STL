#include<iostream>
#include<utility>
using namespace std;
int main(){
	
	pair<int ,float> p3;//defaault construcotr is called
	pair<int ,int > p1(10,20);
	pair<int,string> p2(10,"GFG");
	
	pair<string,char> p5;
	cout<<p1.first<<" "<<p1.second<<"\n";
	cout<<p2.first<<" "<<p2.second<<"\n";
	cout<<p3.first<<" "<<p3.second<<"\n";
	cout<<p5.first<<" "<<p5.second<<"\n";
	
	pair<int,int> s1;
	s1={4,12};
	pair<int ,int > s2(4,0);
	cout<<"Operations on pair"<<"\n";
	cout<<(s1==s2)<<"\n";
	cout<<(s1!=s2)<<"\n";
	cout<<(s1>=s2)<<"\n";
	cout<<(s1<s2)<<"\n";
	
}
